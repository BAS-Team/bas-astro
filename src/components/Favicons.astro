---
// import favIcon from '~/assets/favicons/logo.ico';
import favIconPng from '~/assets/favicons/logo.png';
// import favIconSvg from '~/assets/favicons/apple-touch.svg';
import appleTouchIcon from '~/assets/favicons/logo.png';
---

<link rel="shortcut icon" href={favIconPng.src} />
<link rel="icon" type="image/svg+xml" href={favIconPng.src} />
<link rel="mask-icon" href={favIconPng.src} color="#8D46E7" />
<link rel="apple-touch-icon" sizes="180x180" href={appleTouchIcon.src} />
